    },
    {
      "name": "fetchNotes",
      "type": "method",
      "description": "",
      "body": "$ctrl.loaded = false\r\n\r\n$ctrl.demoTaskApi.demo_api_tasks_services_note_SelectAllNote({\r\n    limit: 1000,\r\n    offset: 0.\r\n}).then(result => {\r\n    $ctrl.notes = result.data.note\r\n})",
      "invokeOnLoad": false,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
