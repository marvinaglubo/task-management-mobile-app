    {
      "name": "onFilterChange",
      "type": "method",
      "description": "",
      "body": "if (item instanceof Event && item.target instanceof HTMLElement) {\r\n    const target = item.target\r\n    const label = target.closest('.menu-item')?.querySelector('.menu-label')?.textContent\r\n\r\n    if (label) {\r\n        $ctrl.filter = label\r\n    }\r\n}\r\n",
      "invokeOnLoad": false,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": [
        "item"
      ]
    },
    {
      "name": "refreshData",
      "type": "method",
      "description": "",
      "body": "$ctrl.loaded = false\n\nawait $ctrl.projectManager.refreshProjects()\nconst { doneProjects, toDoProjects, ongoingProjects, percentage, projects } = $ctrl.projectManager\n\nconst getColors = () => {\n    try {\n        return [\n            getComputedStyle(document.body).getPropertyValue('--color-1'),\n            getComputedStyle(document.body).getPropertyValue('--color-2'),\n            getComputedStyle(document.body).getPropertyValue('--color-3')\n        ]\n    } catch {\n        return ['#F68059', '#FFBF3A', '#4E3EC8']\n    }\n}\n\n$ctrl.charts.overall = {\n    data: [doneProjects.length, toDoProjects.length, ongoingProjects.length],\n    colors: getColors(),\n    legends: [\n        { label: 'Done', value: percentage.done },\n        { label: 'To Do', value: percentage.todo },\n        { label: 'Ongoing', value: percentage.ongoing }\n    ],\n    count: projects.length,\n    countLabel: 'Total Projects'\n}\n\n$ctrl.$timeout(async () => {\n    $ctrl.ongoingProjects = ongoingProjects.filter((_, i) => i < 2)\n\n    for (const project of $ctrl.ongoingProjects) {\n        await $ctrl.projectManager.refreshData({ ...project, data: project, tasks: [] }, true)\n    }\n\n    $ctrl.loaded = true\n})\n",
      "invokeOnLoad": false,
      "isAsync": true,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
      "name": "init",
      "type": "method",
      "description": "",
      "body": "$ctrl.$device.onBackButton(e => {\r\n    e.preventDefault()\r\n})\r\n\r\n$ctrl.refreshData()\r\n$ctrl.pullToRefresh.register('main', () => {\r\n    return $ctrl.refreshData()\r\n})",
      "invokeOnLoad": true,
      "isAsync": true,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
      "name": "onProjectClick",
      "type": "method",
      "description": "",
      "body": "if (event.target?.closest('.avatar')) {\r\n    return\r\n}\r\n\r\n$ctrl.views.project.show(project)",
      "invokeOnLoad": false,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": [
        "event",
        "project"
      ]
    },
