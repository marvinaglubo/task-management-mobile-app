    {
      "name": "getStatusLabel",
      "type": "method",
      "description": "",
      "body": "const status = $ctrl.data?.status\n\nswitch(status) {\n    case 'Completed': return 'Done'\n    case 'In Progress': return 'In Progress'\n    case 'Not Started': return 'Not Started'\n}",
