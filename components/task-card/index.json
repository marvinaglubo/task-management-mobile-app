      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": []
    },
    {
      "name": "getUserFullName",
      "type": "method",
      "description": "",
      "body": "const user = $ctrl.user.getUser($ctrl.data.assignedUserId)\n\nreturn user ? user.firstName + \" \" + user.lastName : \"\"",
