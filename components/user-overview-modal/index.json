      "name": "init",
      "type": "method",
      "description": "",
      "body": "const modal = $ctrl.$element[0]?.closest('.bln-modal')\r\nconst backdrop = modal?.querySelector('.bln-modal-backdrop')\r\n\r\nif (backdrop) {\r\n    backdrop.addEventListener('click', () => {\r\n        $ctrl.onCancelClick()\r\n    })\r\n}",
      "invokeOnLoad": true,
      "isAsync": false,
      "returnType": {
        "name": "any"
      },
      "parameters": []
